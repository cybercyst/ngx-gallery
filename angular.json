{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-gallery": {
      "projectType": "library",
      "root": "projects/ng-gallery",
      "sourceRoot": "projects/ng-gallery/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ng-gallery/tsconfig.lib.json",
            "project": "projects/ng-gallery/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-gallery/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-gallery/src/test.ts",
            "tsConfig": "projects/ng-gallery/tsconfig.spec.json",
            "karmaConfig": "projects/ng-gallery/karma.conf.js"
          }
        }
      }
    },
    "ng-gallery-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ng-gallery-demo",
      "sourceRoot": "projects/ng-gallery-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-gallery-demo",
            "index": "projects/ng-gallery-demo/src/index.html",
            "main": "projects/ng-gallery-demo/src/main.ts",
            "polyfills": "projects/ng-gallery-demo/src/polyfills.ts",
            "tsConfig": "projects/ng-gallery-demo/tsconfig.app.json",
            "assets": [
              "projects/ng-gallery-demo/src/favicon.ico",
              "projects/ng-gallery-demo/src/assets"
            ],
            "styles": [
              "projects/ng-gallery-demo/src/styles.scss",
              "node_modules/highlight.js/styles/a11y-light.css"
            ],
            "allowedCommonJsDependencies": [
              "bezier-easing",
              "@fortawesome/free-brands-svg-icons",
              "@fortawesome/free-solid-svg-icons"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ng-gallery-demo/src/environments/environment.ts",
                  "with": "projects/ng-gallery-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "12kb",
                  "maximumError": "12kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-gallery-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-gallery-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-gallery-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-gallery-demo/src/test.ts",
            "polyfills": "projects/ng-gallery-demo/src/polyfills.ts",
            "tsConfig": "projects/ng-gallery-demo/tsconfig.spec.json",
            "karmaConfig": "projects/ng-gallery-demo/karma.conf.js",
            "assets": [
              "projects/ng-gallery-demo/src/favicon.ico",
              "projects/ng-gallery-demo/src/assets"
            ],
            "styles": [
              "projects/ng-gallery-demo/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ng-gallery-demo/e2e/protractor.conf.js",
            "devServerTarget": "ng-gallery-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-gallery-demo:serve:production"
            }
          }
        }
      }
    },
    "carousel-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/carousel-demo",
      "sourceRoot": "projects/carousel-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/carousel-demo",
            "index": "projects/carousel-demo/src/index.html",
            "main": "projects/carousel-demo/src/main.ts",
            "polyfills": "projects/carousel-demo/src/polyfills.ts",
            "tsConfig": "projects/carousel-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/carousel-demo/src/favicon.ico",
              "projects/carousel-demo/src/assets"
            ],
            "styles": [
              "projects/carousel-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/carousel-demo/src/environments/environment.ts",
                  "with": "projects/carousel-demo/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "carousel-demo:build:production"
            },
            "development": {
              "browserTarget": "carousel-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "carousel-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/carousel-demo/src/test.ts",
            "polyfills": "projects/carousel-demo/src/polyfills.ts",
            "tsConfig": "projects/carousel-demo/tsconfig.spec.json",
            "karmaConfig": "projects/carousel-demo/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/carousel-demo/src/favicon.ico",
              "projects/carousel-demo/src/assets"
            ],
            "styles": [
              "projects/carousel-demo/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "carousel": {
      "projectType": "library",
      "root": "projects/carousel",
      "sourceRoot": "projects/carousel/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/carousel/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/carousel/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/carousel/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/carousel/src/test.ts",
            "tsConfig": "projects/carousel/tsconfig.spec.json",
            "karmaConfig": "projects/carousel/karma.conf.js"
          }
        }
      }
    }
  }
}
